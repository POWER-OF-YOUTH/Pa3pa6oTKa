                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">Студельник</a>
            </div>
            <div class="navbar-left navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-area="" asp-controller="Home" asp-action="TimeTable">Расписание</a></li>
